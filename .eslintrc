{
  "env": {
    "node": true
  },
  "rules": {
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "eqnull": true,
    "forin": true,
    "freeze": true,
    "funcscope": true,
    "immed": true,
    "indent": [
      2,
      2
    ],
    "latedef": true,
    "max-depth": 3,
    "max-len": [
      2,
      100,
      4
    ],
    "max-statements": 15,
    "newcap": true,
    "no-bitwise": true,
    "no-shadow": false,
    "no-underscore-dangle": false,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "quotes": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
  }
}